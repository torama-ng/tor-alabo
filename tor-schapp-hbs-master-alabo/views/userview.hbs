<br>
<div class="container-fluid mt-n1" style="background:#00a2e8; height:100%">
<div class="mt-5">
     
{{#if user}}
    <div class="container mt-5">
        
        <table class="table table-striped table-light mt-5">
            <thead >
                <tr>
                <th scope="col">#</th>
                 <th>Profile picture</th>
                <th scope="col">First Name</th>
                <th scope="col">Last Name</th>
                <th scope="col">Username</th>
                <th scope="col">Email</th>
                <th scope="col">Watch Count</th>
                <th scope="col">User Edit</th>
                <th scope="col"> View Details</th>
        
                </tr>
            </thead>
            <tbody>
                {{#each users}}
                <tr>
                <th scope="row">{{@index}}</th>
                {{#if photourl}}
                <th><img src="{{photourl}}" alt="photo" class="tea"></th>
                {{else}}
                <th><img src="/images/Avatar-dp.jpg" alt="photo" class="tea"></th>
                {{/if}}
                <td>{{firstname}}</td>
                <td>{{lastname}}</td>
                <td>{{username}}</td>
                <td>{{email}}</td>
                <td>{{this.activity.length}}</td>
                <td><a href="/users/userform?id={{this._id}}"> <i class="fas fa-user-edit "></i></a></td>
                <td><a href="/users/details"> <i class="fas fa-eye"></i></a></td>
                </tr>
                {{/each}}
            </tbody>
        </table>
            
       
    </div>
       </div> 
        
    </div> 
{{else}}
     <div class="alert alert-danger">Please login</div>
{{/if}}

